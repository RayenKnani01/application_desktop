   <property name="styleSheet">
    <string notr="true">/* Fond du QTabWidget */
QTabWidget {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(30, 144, 255, 255), stop:1 rgba(0, 70, 128, 255));
}

/* Style des onglets */
QTabBar::tab {
    background-color: #2196F3; /* Bleu primaire du thème de sport nautique */
    color: #ffffff; /* Texte blanc */
    border: 2px solid #1976D2; /* Bordure bleu foncé */
    padding: 12px 20px; /* Espacement interne - ajustez selon vos besoins */
    text-align: center; /* Centrer le texte */
}

/* Style des onglets lorsqu'ils sont survolés */
QTabBar::tab:hover {
    background-color: #1565C0; /* Bleu plus foncé au survol */
}

/* Style des onglets actifs */
QTabBar::tab:selected {
    background-color: #ffffff; /* Fond blanc pour l'onglet actif */
    color: #2196F3; /* Texte bleu primaire pour l'onglet actif */
    border-bottom: 2px solid #2196F3; /* Bordure bleu primaire en bas de l'onglet actif */
}

/* Style du contenu des onglets */
QTabWidget::pane {
    border: 2px solid #1976D2; /* Bordure bleu foncé pour le contenu des onglets */
    border-top: 0; /* Supprimer la bordure supérieure */
}

/* Style des boutons de fermeture des onglets */
QTabBar::close-button {
    image: url(close.png); /* Remplacez par le chemin de votre icône de fermeture */
    subcontrol-position: right; /* Position à droite du texte de l'onglet */
}

QTabBar::close-button:hover {
    image: url(close-hover.png); /* Icône de fermeture au survol */
}

/* Ajustement de la police */
QTabBar {
    font-size: 16px; /* Taille de la police pour les onglets */
    qproperty-drawBase: 0; /* Désactiver le dessin de la base (fond) de l'onglet */
}
</string>
